{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"title": "Person",
	"type": "object",
	"properties": {
		"name": {
			"type": "string"
		},
		"age": {
			"type": "integer",
			"minimum": 0
		},
		"email": {
			"type": "string",
			"format": "email"
		}
	},
	"required": ["name", "email"]
}
